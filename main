class Vehicles:

    def __init__(self, name, max_speed, mileage):
        self.name= name
        self. max_speed= max_speed
        self.mileage=mileage

class bus(Vehicles):
    pass

school_bus=bus("volvo", 120, 13)
print("Vehicle name:", school_bus.name,"speed is", school_bus.max_speed, "mileage", school_bus.mileage)
